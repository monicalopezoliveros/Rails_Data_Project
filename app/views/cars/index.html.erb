<h1>Cars Available for Rent</h1>
<div class="cars-list">
    <ul>
        <% @cars.each do |car| %>
            <li>
                <%= car.brand %> <%= car.model %> -
                <%= link_to "See details", car_path(car) %>
            </li>
        <% end %>
    </ul>

    <%= paginate @cars %>
</div>
